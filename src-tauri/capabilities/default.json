{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*"
        }
      ],
      "deny": [
        {
          "url": "https://private.tauri.app"
        }
      ]
    },
    "opener:allow-default-urls",
    "sql:default",
    "sql:allow-select",
    "sql:allow-execute",
    "store:default",
    "dialog:default",
    "shell:default"
  ]
}