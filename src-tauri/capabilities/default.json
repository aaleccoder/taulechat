{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.tauri.app"
        },
        {
          "url": "https://openrouter.ai/api/v1/chat/completions"
        }
      ],
      "deny": [
        {
          "url": "https://private.tauri.app"
        }
      ]
    },
    "sql:default",
    "sql:allow-select",
    "sql:allow-execute",
    "store:default"
  ]
}